{
  "manifest_version": 2,

  "name": "标签页暂存",
  "description": "暂存当前标签页",
  "version": "1.0.0",

  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" ,

  "permissions": [
    "activeTab"
  ]
}
